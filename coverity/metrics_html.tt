    <title>
      SW DEFECT METRICS for [%- project -%] [%- hardware -%]
    </title>

    <meta name = "viewport" content = "initial-scale = 1, user-scalable = no">

    <script src="../../../../GitHub/Chart.js/Chart.js"></script>

    <script type="text/javascript">
      function drawLegend1()
      {
        // Set the starting x and y values
        var x=5;
        var y=5;

        // Get the area for the canvas and the 2D context
        // Draw 2 rectangles each with label

        var ctx = document.getElementById("criticalLegend").getContext("2d");

        ctx.fillStyle = "rgb(0,0,0)";
        ctx.font      = "9pt Arial";
        ctx.fillText( "LEGEND:", x+45, y+23 );

        ctx.fillStyle = "rgba(255,0,0,1)";
        ctx.fillRect( x+110, y+10, 25, 15 );

        ctx.fillStyle = "rgb(0,0,0)";
        ctx.font      = "9pt Arial";
        ctx.fillText( "C++", x+140, y+23 );

        ctx.fillStyle = "rgba(0,0,255,1)";
        ctx.fillRect( x+175, y+10, 25, 15 );

        ctx.fillStyle = "rgb(0,0,0)";
        ctx.font      = "9pt Arial";
        ctx.fillText( "JAVA", x+205, y+23 );
      }

      function drawLegend2()
      {
        // Set the starting x and y values
        var x=5;
        var y=5;

        // Get the area for the canvas and the 2D context
        // Draw 2 rectangles each with label

        var ctx = document.getElementById("majorLegend").getContext("2d");

        ctx.fillStyle = "rgba(155,0,0,1)";
        ctx.fillRect( x+100, y+10, 25, 15 );

        ctx.fillStyle = "rgb(0,0,0)";
        ctx.font      = "9pt Arial";
        ctx.fillText( "C++", x+135, y+20 );

        ctx.fillStyle = "rgba(0,0,155,1)";
        ctx.fillRect( x+100, y+40, 25, 15 );

        ctx.fillStyle = "rgb(0,0,0)";
        ctx.font      = "9pt Arial";
        ctx.fillText( "JAVA", x+135, y+50 );
      }

    </script>

    <style>
      canvas   {
	       }
     .headline {
                text-align:center;
                font-weight:bold;
                font-size:40px;
               }
     .heading {
               text-align:center;
               font-weight:bold;
              }
     .data    {
               text-align:center;
              }
   </style>
  </head>

  <body>

    <h1 class="headline">SW DEFECT METRICS for [%- project -%] [%- hardware -%]</h1>
    <table border=1 width="100%">
      <tr>
        <td>&nbsp;</td>
        <td colspan=3; class="heading">CRITICAL</td>
        <td colspan=3; class="heading">MAJOR</td>
        <td colspan=3; class="heading">MINOR</td>
      </tr>
      <tr>
      <td class="heading">Date</td> <td class="heading">GPP C++</td> <td class="heading">MODEM C++</td> <td class="heading">GPP Java</td> <td class="heading">GPP C++</td> <td class="heading">MODEM C++</td> <td class="heading">GPP Java</td> <td class="heading">GPP C++</td> <td class="heading">MODEM C++</td> <td class="heading">GPP Java</td></tr>

      [% FOREACH colordate = colordates -%]
        <tr>
          <td class="data">[%- colordate.DATE -%]</td>
          <td class="data">[%- colordate.CRITICAL_PA_CPP -%]</td> <td class="data">[%- colordate.CRITICAL_PB_CPP -%]</td> <td class="data">[%- colordate.CRITICAL_PA_JAVA -%]</td>
          <td class="data">[%- colordate.MAJOR_PA_CPP -%]</td> <td class="data">[%- colordate.MAJOR_PB_CPP -%]</td> <td class="data">[%- colordate.MAJOR_PA_JAVA -%]</td>
          <td class="data">[%- colordate.MINOR_PA_CPP -%]</td> <td class="data">[%- colordate.MINOR_PB_CPP -%]</td> <td class="data">[%- colordate.MINOR_PA_JAVA -%]</td>
        </tr>
      [%- END -%]
    </table>

    <hr>

    <h2>CRITICAL BURNDOWN</h2>

    <canvas id="canvasCritical" height="300" width="600"></canvas>

    <script>
      var lineChartCritical = {
                                labels:[
                                [%- FOREACH date = dates -%]"
                                  [%- date.DATE -%]"
                                  [%- "," UNLESS loop.last -%]
                                [%- END -%]],
                                datasets :  [
                                              {
                                                fillColor        : "rgba(255,255,204,0.5)",
                                                strokeColor      : "rgba(255,0,0,1)",
                                                pointColor       : "rgba(255,0,0,1)",
                                                pointStrokeColor : "#fff",
                                                data:[
                                                [%- FOREACH date = dates -%]
                                                  [%- date.CRITICAL_PA_CPP -%][%- "," UNLESS loop.last -%]
                                                [%- END -%]]
                                              },
                                              {
                                                fillColor        : "rgba(255,255,204,0.5)",
                                                strokeColor      : "rgba(0,0,255,1)",
                                                pointColor       : "rgba(0,0,255,1)",
                                                pointStrokeColor : "#fff",
                                                data:[
                                                [%- FOREACH date = dates -%]
                                                  [%- date.CRITICAL_PA_JAVA -%][%- "," UNLESS loop.last -%]
                                                [%- END -%]]
                                              }
                                            ]
                              }

      var myLine = new Chart(document.getElementById("canvasCritical").getContext("2d")).Line(lineChartCritical);

    </script>

    <div id="legend1" style="width:100%; height:10%;">
      <canvas id="criticalLegend" height="100" width="600"></canvas>
    </div>

    <script type="text/javascript">
      window.onload = drawLegend1();
    </script>

    <hr>

    <h2>MAJOR BURNDOWN</h2>
    <canvas id="canvasMajor" height="300" width="600"></canvas>

    <script>
      var lineChartMajor = {
                            labels:[[%- FOREACH date = dates -%]"[%- date.DATE -%]"[%- "," UNLESS loop.last -%][%- END -%]],
                            datasets :  [
                                          {
                                            fillColor        : "rgba(255,255,204,0.5)",
                                            strokeColor      : "rgba(153,0,0,1)",
                                            pointColor       : "rgba(153,0,0,1)",
                                            pointStrokeColor : "#fff",
                                            data:[
                                            [%- FOREACH date = dates -%]
                                              [%- date.MAJOR_PA_CPP -%][%- "," UNLESS loop.last -%]
                                            [%- END -%]]
                                          },
                                          {
                                            fillColor        : "rgba(255,255,204,0.5)",
                                            strokeColor      : "rgba(0,0,153,1)",
                                            pointColor       : "rgba(0,0,153,1)",
                                            pointStrokeColor : "#fff",
                                            data:[
                                            [%- FOREACH date = dates -%]
                                              [%- date.MAJOR_PA_JAVA -%][%- "," UNLESS loop.last -%]
                                            [%- END -%]]
                                          }
                                        ]
                          }

      var myLine = new Chart(document.getElementById("canvasMajor").getContext("2d")).Line(lineChartMajor);

    </script>

    <div id="legend1" style="width:100%; height:10%;">
      <canvas id="majorLegend" height="100" width="600"></canvas>
    </div>

    <script type="text/javascript">
      window.onload = drawLegend2();
    </script>

    <div id="footer">
      <div id="copyright">
        &copy; [% copyright %]
      </div>
    </div>

  </body>
</html>
